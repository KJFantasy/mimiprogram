/* pages/lookAround/lookAround.wxss */
page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates_container {
  height: calc( 100vh - 90rpx);
  display: flex;
}
.cates .cates_container .left_menu {
  flex: 2;
}
.cates .cates_container .left_menu .item_content {
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  color: #555555;
  background-color: #f2f2f2;
}
.cates .cates_container .right_content {
  flex: 5;
}
